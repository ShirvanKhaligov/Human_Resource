@model RecruitmentCandidateVM
@{
    ViewData["Title"] = "Create";
}

<style>


    .btn-success {
        background-color: #f6822d !important;
        border-color: #f6822d !important;
    }

    .btn-primary {
        background-color: #f6822d !important;
        border-color: #f6822d !important;
    }

    .btn-warning {
        background-color: #f6822d !important;
        border-color: #f6822d !important;
    }

    .btn-danger {
        background-color: #f6822d !important;
        border-color: #f6822d !important;
    }

    .btn-info {
        background-color: #f6822d !important;
        border-color: #f6822d !important;
    }
</style>

<!--WRAPPER START -->
<div class="content-wrapper" style="color:black; background-color: #fff;">
    <br>
    <div class="container-fluid">
        <div class="bs-example">
            <nav class="navbar navbar-expand-md navbar-light bg-gray-light">

                <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#navbarCollapse">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse col-md-12 mx-auto" id="navbarCollapse">
                    <div class="navbar-nav">
                        <a onclick="myFunction1()" style="font-family: 'Roboto';" class="nav-link active"
                           href="#">Şəxsi məlumatlar</a>
                        <a onclick="myFunction2()" style="font-family: 'Roboto'; " class="nav-link"
                           href="#">Təhsil haqqında</a>
                        <a onclick="myFunction3()" style="font-family: 'Roboto'; " class="nav-link"
                           href="#">Dil bilikləri</a>
                        <a onclick="myFunction4()" style="font-family: 'Roboto'; " class="nav-link"
                           href="#">Kompyuter bilikləri</a>
                        <a onclick="myFunction5()" style="font-family: 'Roboto'; " class="nav-link"
                           href="#">Ailə üzvləri</a>
                        <a onclick="myFunction6()" style="font-family: 'Roboto';" class="nav-link"
                           href="#">Əlavə məlumatlar</a>
                        <a onclick="myFunction7()" style="font-family: 'Roboto';" class="nav-link"
                           href="#">Əmrlərin skan forması</a>
                    </div>
                </div>
            </nav>
        </div>



        <div style="" class="col-md-12 ">
            <br>
            <form method="post" enctype="multipart/form-data">
                <div class="siema">
                    <div style="display: block" id="step-one">
                        <div class="row">
                            <div class="col-md-9">
                                <div class="form-group row ml-md-5 ">
                                    <label for="name" class="col-md-3 ">Adı:</label>
                                    <div class="col-sm-5">
                                        @foreach (var empName in Model.Emplooyees)
                                        {
                                            <input type="text" readonly style="height: 27px;" value="@empName.FirstName" class="form-control" id="name" name="name">
                                        }
                                    </div>
                                </div>

                                <div class="form-group row ml-md-5 ">
                                    <label for="surname" class="col-md-3">Soyadı:</label>
                                    <div class="col-sm-5">
                                        @foreach (var empName in Model.Emplooyees)
                                        {
                                            <input type="text" readonly style="height: 27px;" value="@empName.Lastname" class="form-control" id="surname" name="surname">
                                        }
                                    </div>
                                </div>
                                <div class="form-group row ml-md-5 ">
                                    <label for="fatherName" class="col-md-3">Ata adı:</label>
                                    <div class="col-sm-5">
                                        @foreach (var empName in Model.Emplooyees)
                                        {
                                            <input type="text" readonly style="height: 27px;" value="@empName.FatherName" class="form-control" id="fatherName" name="fatherName">
                                        }
                                    </div>
                                </div>

                                <div class="form-group row ml-md-5 ">
                                    <label for="idCard" class="col-md-3">Ş\V:</label>
                                    <div class="col-sm-5">
                                        <input asp-for="@Model.IdCard" type="text" maxlength="13" style="height: 27px;text-transform:uppercase;" class="form-control" id="idCard">
                                        <span asp-validation-for="@Model.IdCard" class="text-danger text-bold" style="font-size:14px;"></span>
                                    </div>
                                </div>
                                <div class="form-group row ml-md-5 ">
                                    <label for="finCode" class="col-md-3">FİN:</label>
                                    <div class="col-sm-5">
                                        <input asp-for="@Model.FinCode" type="text" style="height: 27px;text-transform:uppercase;" maxlength="7" class="form-control" id="finCode">
                                        <span asp-validation-for="@Model.FinCode" class="text-danger text-bold" style="font-size:14px;"></span>
                                    </div>
                                </div>

                                <div class="form-group  row ml-md-5 m-0">
                                    <label class="col-md-3">Hərbi mükəlləfiyyəti:</label>
                                    <div class="col-sm-2 ">
                                        <label class="text-success">Var</label>
                                        <input onclick="checkBox()" type="radio" value="yess" name="MilitaryStatus" id="yesMilitaryStatus">
                                    </div>
                                    <div class="col-sm-2">
                                        <label class="text-danger">Yoxdur</label>
                                        <input onclick="checkBox1()" checked type="radio" value="noo" name="MilitaryStatus" id="noMilitaryStatus">
                                    </div>
                                </div>
                                @if (ViewData["error_military"] != null)
                                {
                                    <span style="margin-left:245px" class="text-danger">@ViewData["error_military"]</span>
                                }
                                @if (ViewData["error_militaryRank"] != null)
                                {
                                    <span style="margin-left:245px" class="text-danger">@ViewData["error_militaryRank"]</span>
                                }
                                <div style="display: none" id="military">
                                    <div class="form-group row ml-md-5">
                                        <label class="col-md-3" for="militaryRank">Hərbi rütbəsi:</label>
                                        <div class="col-sm-5">
                                            <select style="padding: 0px" class="form-control input-height" name="military"
                                                    id="militaryRank">
                                                <option selected disabled>Seç</option>
                                                @foreach (var rank in Model.MillitaryRanks)
                                                {
                                                    <option value="@rank.Id">@rank.Name</option>
                                                }
                                            </select>

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div style="margin-top: -51px;" class="col-md-3">
                                <div class="avatar-upload">
                                    @if (Model.AppUser.Image != null)
                                    {
                                        foreach (var em in Model.Emplooyees)
                                        {
                                            if (em.Id == Model.AppUser.EmplooyeeId)
                                            {
                                                <div class="avatar-edit">
                                                    <input accept="image/*" asp-for="@Model.Photo" id="imageUpload" />
                                                    <label for="imageUpload"></label>
                                                </div>
                                                <div class="avatar-preview">
                                                    <div id="imagePreview" style="background-image: url(/allUserImg/@Model.AppUser.Image)">
                                                    </div>
                                                </div>
                                                <span class="text-danger text-bold" style="font-size:14px;" asp-validation-for="@Model.Photo"></span>
                                            }
                                            else
                                            {
                                                <div class="avatar-edit">
                                                    <input accept="image/*" asp-for="@Model.Photo" id="imageUpload" />
                                                    <label for="imageUpload"></label>
                                                </div>
                                                <div class="avatar-preview">
                                                    <div id="imagePreview" style="background-image: url(/dist/img/user.png)">
                                                    </div>
                                                </div>
                                                <span class="text-danger text-bold" style="font-size:14px;" asp-validation-for="@Model.Photo"></span>
                                            }
                                        }

                                    }
                                    else
                                    {
                                        <div class="avatar-edit">
                                            <input accept="image/*" asp-for="@Model.Photo" id="imageUpload" />
                                            <label for="imageUpload"></label>
                                        </div>
                                        <div class="avatar-preview">
                                            <div id="imagePreview" style="background-image: url(/dist/img/user.png)">
                                            </div>
                                        </div>
                                        <span class="text-danger text-bold" style="font-size:14px;" asp-validation-for="@Model.Photo"></span>
                                    }

                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-11 mx-auto">
                                <span class="text-danger text-bold" style="font-size:14px;" asp-validation-for="Word"></span>
                                <span class="text-danger text-bold" style="font-size:14px;" asp-validation-for="Pdf"></span>
                                <span class="text-danger text-bold" style="font-size:14px;" asp-validation-for="PhotoCertificate"></span>
                            </div>
                            <div class="col-md-11 mx-auto">
                                <button style="float: right;width: 150px;margin-right: 35px;" id="next" onclick="myFunction2()" type="button"
                                        class="btn btn-success">
                                    İrəli
                                </button>
                                <button style="margin-top: 50px;" type="button" class="btn btn-danger">
                                    <a asp-controller="Practitioner" asp-action="AllPractitioner" style="padding: 5px 100px;color:white;">İmtina</a>
                                </button>
                            </div>
                        </div>
                    </div>
                    <!--STEP TWO-->
                    <div style="display: none" id="step-two" class="row">
                        <!--Candidate Education Start-->
                        <div class="property-fields__rows mb-5">
                            @if (Model.School != null)
                            {
                                <div style="border:1px solid;color: black">
                                    <div>
                                        <div class="form-group row ml-md-5 mt-3">
                                            <div class="col-md-2">
                                                <label for="educationType">Orta təhsil:</label>
                                            </div>
                                            <div class="col-sm-5">
                                                <input type='text' value="@Model.School.Name" readonly class="form-control" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }
                            @if (Model.EducationsToEmployees.Count() != 0)
                            {
                                @foreach (var educType in Model.EducationsToEmployees.Where(e => e.Education.Status == true))
                                {
                                    <div>
                                        <div style="border:1px solid;color: black">
                                            <div>
                                                <div class="form-group row ml-md-5 mt-3">
                                                    <div class="col-md-2">
                                                        <label for="educationType">Təhsilin dərəcəsi:</label>
                                                    </div>
                                                    <div class="col-sm-5">
                                                        <input type='text' value="@educType.Education.Profession.UniFaculty.EducationTypeToUniversity.EducationType.Name" readonly class="form-control" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="line-item-property__field line-item-property__universityName">
                                                <div class="form-group row ml-md-5">
                                                    <div class="col-md-2">
                                                        <label for="universityName">Təhsil ocağının adı:</label>
                                                    </div>
                                                    <div class="col-sm-5">
                                                        <input type='text' value="@educType.Education.Profession.UniFaculty.EducationTypeToUniversity.Universities.Name" readonly class="form-control" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <div class="line-item-property__field line-item-property__enteringTime">
                                                        <div class="form-group row ml-md-5">
                                                            <div class="col-sm-7">
                                                                <label required for="enteringTime">Başlama tarixi:</label>
                                                            </div>
                                                            <div class="col-sm-5">
                                                                <input type='text' value="@educType.Education.EnteringTime.ToString("dd/MM/yyyy")" readonly class="form-control" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-4 ">
                                                    <div class="line-item-property__field line-item-property__endTime">
                                                        <div class="form-group row  ml-md-5 ">
                                                            <div class="col-md-4">
                                                                <label required for="endTime">Bitmə tarixi:</label>
                                                            </div>
                                                            <div class="col-sm-5">
                                                                <input type='text' value="@educType.Education.FinishTime.ToString("dd/MM/yyyy")" readonly class="form-control" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="line-item-property__field line-item-property__facultyName">
                                                <div class="form-group row ml-md-5">
                                                    <div class="col-md-2">
                                                        <label for="facultyName">Fakültə:</label>
                                                    </div>
                                                    <div class="col-sm-5">
                                                        <input type='text' value="@educType.Education.Profession.UniFaculty.Name" readonly class="form-control" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="line-item-property__field line-item-property__specialtyName">
                                                <div class="form-group row ml-md-5">
                                                    <div class="col-md-2">
                                                        <label for="specialtyName">İxtisas:</label>
                                                    </div>
                                                    <div class="col-sm-5">
                                                        <input type='text' value="@educType.Education.Profession.Name" readonly class="form-control" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="line-item-property__field line-item-property__AcceptHon">
                                                <div class="form-group row ml-md-5">
                                                    <div class="col-md-2">
                                                        <label>Qəbul balı:</label>
                                                    </div>
                                                    <div class="col-sm-5">
                                                        <input type='text' value="@educType.Education.AcceptHoney" readonly class="form-control" />
                                                    </div>
                                                </div>
                                            </div>
                                            <br>
                                        </div><br><br>
                                    </div>
                                }
                            }
                        </div>
                        <div class="property-fields__rows">
                            <div id="educationTable" class="educTableDiv">
                                @if (Model.EducationsToEmployeesForFalse != null)
                                {
                                    @foreach (var educType in Model.EducationsToEmployeesForFalse)
                                    {
                                        <div>
                                            <div style="border:1px solid;color: black ">
                                                <div>
                                                    <div class="form-group row ml-md-5 mt-3">
                                                        <div class="col-md-2">
                                                            <label for="educationType">Təhsilin dərəcəsi:</label>
                                                        </div>
                                                        <div class="col-sm-5">
                                                            <input type='text' value="@educType.Education.Profession.UniFaculty.EducationTypeToUniversity.EducationType.Name" readonly class="form-control" />
                                                        </div>
                                                        <div class='col-sm-4' style='text-align:end;'>
                                                            <button type='button' onclick='modalEducationDelete(@educType.Emplooyee.Id,@educType.Education.Id)' style='padding:6px 70px;' class='btn btn-danger'>Sil</button>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="line-item-property__field line-item-property__universityName">
                                                    <div class="form-group row ml-md-5">
                                                        <div class="col-md-2">
                                                            <label for="universityName">Təhsil ocağının adı:</label>
                                                        </div>
                                                        <div class="col-sm-5">
                                                            <input type='text' value="@educType.Education.Profession.UniFaculty.EducationTypeToUniversity.Universities.Name" readonly class="form-control" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="line-item-property__field line-item-property__enteringTime">
                                                            <div class="form-group row ml-md-5">
                                                                <div class="col-sm-7">
                                                                    <label required for="enteringTime">Başlama tarixi:</label>
                                                                </div>
                                                                <div class="col-sm-5">
                                                                    <input type='text' value="@educType.Education.EnteringTime.ToString("yyyy-MM-dd")" readonly class="form-control" />
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4 ">
                                                        <div class="line-item-property__field line-item-property__endTime">
                                                            <div class="form-group row  ml-md-5 ">
                                                                <div class="col-md-4">
                                                                    <label required for="endTime">Bitmə tarixi:</label>
                                                                </div>
                                                                <div class="col-sm-5">
                                                                    <input type='text' value="@educType.Education.FinishTime.ToString("yyyy-MM-dd")" readonly class="form-control" />
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="line-item-property__field line-item-property__facultyName">
                                                    <div class="form-group row ml-md-5">
                                                        <div class="col-md-2">
                                                            <label for="facultyName">Fakültə:</label>
                                                        </div>
                                                        <div class="col-sm-5">
                                                            <input type='text' value="@educType.Education.Profession.UniFaculty.Name" readonly class="form-control" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="line-item-property__field line-item-property__specialtyName">
                                                    <div class="form-group row ml-md-5">
                                                        <div class="col-md-2">
                                                            <label for="specialtyName">İxtisas:</label>
                                                        </div>
                                                        <div class="col-sm-5">
                                                            <input type='text' value="@educType.Education.Profession.Name" readonly class="form-control" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class='line-item-property__field line-item-property__AcceptHon'>
                                                    <div class='form-group row ml-md-5'>
                                                        <div class='col-md-2'>
                                                            <label>Qəbul balı:</label>
                                                        </div>
                                                        <div class='col-sm-5'>
                                                            <input type='number' value="@educType.Education.AcceptHoney" readonly class="form-control" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <br>
                                            </div><br><br>
                                        </div>
                                    }
                                }
                            </div>
                        </div>
                        <!--Candidate Education End-->
                        <!----------------------------------------------------------Education New------------------------------------------------------------------------->

                        <div class="property-fields__rows">
                            <div id="property-fields__row-1" class="property-fields__row property-fields__row-item">
                                <div style="border:1px solid;color: black ">
                                    <div class="line-item-property__field line-item-property__educationType">
                                        <div class="form-group row ml-md-5 mt-3">
                                            <div class="col-md-2">
                                                <label for="educationTypeId">Təhsilin dərəcəsi:</label>
                                            </div>
                                            <div class="col-sm-5">
                                                <select onchange="getUniversityRecruitment()" required style="padding: 0px" class="form-control input-height"
                                                        id="educationTypeId">
                                                    <option value="">Seçin</option>
                                                    @foreach (var newEducType in Model.EducationTypes)
                                                    {
                                                        if (newEducType.Name == "Orta")
                                                        {
                                                            <option value="@newEducType.Id" style="display:none">@newEducType.Name</option>
                                                        }
                                                        else
                                                        {
                                                            <option value="@newEducType.Id">@newEducType.Name</option>
                                                        }

                                                    }
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="line-item-property__field line-item-property__universityName">
                                        <div class="form-group row ml-md-5">
                                            <div class="col-md-2">
                                                <label for="universityId">Təhsil ocağının adı:</label>
                                            </div>
                                            <div class="col-sm-5" id="uniResponseDiv">
                                                <select required style="padding: 0px" class="form-control input-height"
                                                        id="universityId_0">
                                                    <option selected disabled value="">Seçin</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="line-item-property__field line-item-property__time">
                                        <div class="form-group row ml-md-5">
                                            <div class="col-md-4">
                                                <div class="form-group row">
                                                    <div class="col-sm-6">
                                                        <label for="startTimeEducation">Başlama tarixi:</label>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <input type="date" name="startTimeEducation" id="startTimeEducation"
                                                               class="form-control input-height" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-4 ">
                                                <div class="form-group row">
                                                    <div class="col-md-3">
                                                        <label for="endTimeEducation">Bitmə tarixi:</label>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <input type="date" name="endTimeEducation" id="endTimeEducation"
                                                               class="form-control input-height" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="line-item-property__field line-item-property__facultyName">
                                        <div class="form-group row ml-md-5">
                                            <div class="col-md-2">
                                                <label for="facultyId">Fakültə:</label>
                                            </div>
                                            <div class="col-sm-5" id="facResponseDiv">
                                                <select required style="padding: 0px" class="form-control input-height"
                                                        id="facultyId_0">
                                                    <option value="">Seçin</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="line-item-property__field line-item-property__specialtyName">
                                        <div class="form-group row ml-md-5">
                                            <div class="col-md-2">
                                                <label for="professionId">İxtisas:</label>
                                            </div>
                                            <div class="col-sm-5" id="proResponseDiv">
                                                <select required style="padding: 0px" class="form-control input-height"
                                                        id="professionId_0">
                                                    <option value="">Seç</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="line-item-property__field line-item-property__AcceptHon">
                                        <div class="form-group row ml-md-5">
                                            <div class="col-md-2">
                                                <label for="accepthoney">Qəbul balı:</label>
                                            </div>
                                            <div class="col-sm-5">
                                                <input required type='number' class="form-control input-height" id="accepthoney" />
                                            </div>
                                        </div>
                                    </div>
                                    <br>
                                </div><br><br>
                            </div>
                        </div>
                        <!----------------------------------------------------------Education New------------------------------------------------------------------------->

                        <div class="line-item-property__actions">
                            <div class="row ">

                                <div class="col-md-10 mx-auto text-center ">
                                    @foreach (var empid in Model.Emplooyees)
                                    {
                                        <input class="btn btn-success col-sm-4" type="button"
                                               onclick="addEduc(@empid.Id)"
                                               value="Yüklə" />
                                    }

                                </div>
                            </div>
                        </div>

                        <br><br>
                        <div class="row">
                            <div class="col-md-10 mx-auto mb-5">
                                <button style="float: right;width: 150px" id="next" onclick="myFunction3()" type="button"
                                        class="btn btn-success">
                                    İrəli
                                </button>
                                <button style="float: left;width: 150px" id="prev" onclick="myFunction1()" type="button"
                                        class="btn btn-danger">
                                    Geri
                                </button>
                            </div>
                        </div>
                    </div>
                    <!--STEP THREE-->
                    <div style="display: none" id="step-three" class="row">
                        <div class="property-fields___rows">
                            <div id="property-fields___row-1" class="property-fields___row property-fields___row-item">
                                <div class="line-item-property___field line-item-property___language">
                                    <div id="languageTable" class="langTableDiv">
                                        <!---->
                                        @if (Model.LanguageToEmployeesForFalse != null)
                                        {
                                            @foreach (var lg in Model.LanguageToEmployeesForFalse)
                                            {
                                                <div class="form-group row">
                                                    <div class="col-md-3 offset-3">
                                                        <div class="row">
                                                            <div class="col-md-3">
                                                                <label for="language">Dilin adı:</label>
                                                            </div>
